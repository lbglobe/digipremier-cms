{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "name": "blog"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "detail": {
      "type": "richtext",
      "required": true
    },
    "image": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "metaTitle": {
      "type": "string"
    },
    "metaKeyword": {
      "type": "string"
    },
    "metaDescription": {
      "type": "string"
    }
  }
}
